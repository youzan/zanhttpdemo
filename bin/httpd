#!/usr/bin/env /usr/local/bin/php
<?php

$cmd = 'start';

switch ($cmd) {
    case 'start':

        /** @var \Zan\Framework\Foundation\Application $app */
        $app = require_once __DIR__.'/../init/app.php';

        $server = $app->createHttpServer();
        $server->start();

        break;
    case 'stop':
        break;
}

